body
	color textColor
	if !!textColor2 && textColor != textColor2 && textColor2!='textColor2'
		color textColor2

	background-color backColor
	if !!backColor2 && backColor != backColor2 && backColor2!='backColor2'
		background-color backColor2

.container
	position	relative

.pages
	left		0
	right		0
	width		auto
	height		650px
	border		1px solid #302F2F
	margin		100px 0 10px 0
	text-align	center
	border-radius 5px
	transition	all 1s

.pages:first-child
	margin		10px 0 10px 0

.pages.active
	border		1px solid gray

.loadingtext
	display		none

body.preload .loadingtext
	display		block

body.preload .intotext
	display		none
