body
	color textColor
	if !!textColor2 && textColor != textColor2 && textColor2!='textColor2'
		color textColor2

	background-color backColor
	if !!backColor2 && backColor != backColor2 && backColor2!='backColor2'
		background-color backColor2

.pages
	position	relative
	width		auto
	height		650px
	border		1px solid red
	margin		10px 0 100px 0
	text-align	center

.loadingtext
	display		none

body.preload .loadingtext
	display		block

body.preload .intotext
	display		none
