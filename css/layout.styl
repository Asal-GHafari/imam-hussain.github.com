body
	color textColor
	if !!textColor2 && textColor != textColor2 && textColor2!='textColor2'
		color textColor2

	background-color backColor
	if !!backColor2 && backColor != backColor2 && backColor2!='backColor2'
		background-color backColor2

body.preload

	.pages
		visibility	hidden

		&:first-child
			visibility	visible

		.loadingtext
			display		block

		.intotext
			display		none

.pages
	left		0
	right		0
	width		950px
	min-height	650px
	border		1px solid #302F2F
	margin		20px auto 80px auto
	text-align	center
	border-radius 5px
	transition border 500ms

	&:first-child
		//margin		10px 0 10px 0

	&.active
		border		1px solid gray

	.loadingtext
		display		none

	h1
		opacity 0

	.p1
		.tx1
			position	relative


.redhl
	color	#E68686